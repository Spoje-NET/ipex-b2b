{
    "name": "spoje.net/ipexb2b",
    "description": "PHP Based Library for easy interaction with IPEX.cz JSON api",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "ext-curl": "*",
        "vitexsoftware/ease-framework": ">=1.3"
    },
    "autoload": {
        "psr-4": {
            "IPEXB2B\\": "src/IPEXB2B/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\IPEXB2B\\": "testing/src/IPEXB2B/",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "phpunit/php-token-stream": "~1.2",
        "phpunit/phpunit": "5.3.*",
        "phpunit/phpunit-skeleton-generator": "*",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "jakub-onderka/php-console-highlighter": "^0.3.2"
    }
}
