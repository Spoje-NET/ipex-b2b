{
  "name": "deb/ipexb2b",
  "description": "PHP Library for interaction with restapi.ipex.cz",
  "homepage": "https://github.com/Spoje-NET/ipex-b2b",
  "license": "GPL-2.0+",
  "version": "0.5",
  "type": "library",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "IPEXB2B\\": "."
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/Ease",
      "options": {
        "symlink": true
      }
    }
  ]
}
